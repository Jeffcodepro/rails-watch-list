<nav class="navbar bg-body-tertiary">
  <div class="container-fluid">
    <i class="fa-solid fa-film"></i>
    <form class="d-flex">
      <button type="button"class= "btn btn-outline-primary" ><%= link_to "New list", new_list_path, class: "text-decoration-none" %></button>
    </form>
  </div>
</nav>
<div class="card text-bg-dark mb-4">
  <%= image_tag 'https://img.freepik.com/fotos-gratis/auditorio-de-cinema-vazio-com-cadeiras_651396-2891.jpg?semt=ais_hybrid&w=740', style: "height: 700px;" %>
  <div class="card-img-overlay d-flex flex-column justify-content-center align-items-center text-center">
    <h4 class="card-title text-dark">Save any kind of movies</h5>
    <p class="card-text text-dark">Favorites</p>
  </div>
</div>

<% images = [
  "data:image/jpeg;base64,",
  "https://st2.depositphotos.com/1006006/7120/i/450/depositphotos_71202953-stock-photo-spotlight-on-gold-star-on.jpg",
  "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRTk7sczCxzs66VyShCSBH14onbNi0i3H03SA&s",
] %>

<div class="container">
  <div class="d-flex justify-content-between">
    <h1>My lists</h1>
    <p><%= link_to "Create a movie list", new_list_path, class: "btn btn-success" %></p>
  </div>
  <div class="row">
    <% @lists.each_with_index do |list, index| %>
      <div class="col-12 col-sm-6 col-md-4 mb-3">
        <div class="card text-bg-dark">
          <%= link_to list_path(list) do %>
            <%= image_tag images[index % images.size], style: "width: 100%; height: 200px; object-fit: cover;" %>
            <div class="d-flex justify-content-center align-items-center position-absolute top-0 start-0 w-100 h-100 custom-overlay">
              <h4 class="bg-dark bg-opacity-50 p-2 rounded text-white"><%= list.name %></h4>
            </div>
          <% end %>
        </div>
      </div>
    <% end %>
  </div>
</div>
